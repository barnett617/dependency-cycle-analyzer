{
  "version": 2,
  "buildCommand": "pnpm --filter @dependency-cycle-analyzer/web build",
  "outputDirectory": "packages/web/dist",
  "framework": "vite",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
